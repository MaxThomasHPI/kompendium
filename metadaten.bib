@article{kaminskienemetetstpbshe2023,
  title = {Mobile Eye Tracking Evoked Teacher Self-Reflection about Teaching Practices and Behavior towards Students in Higher Education},
  author = {Kaminskien{\.e}, Lina and Horlenko, Kateryna and Matulaitien{\.e}, Jovita and Ponomarenko, Tetiana and Rutkien{\.e}, Au{\v s}ra and {Tandzegolskien{\.e}-Bielaglov{\.e}}, Ilona},
