@article{moshamscpdrrdrheisr2023,
  title = {Metadata {{Standard}} for {{Continuous Preservation}}, {{Discovery}}, and {{Reuse}} of {{Research Data}} in {{Repositories}} by {{Higher Education Institutions}}: {{A Systematic Review}}},
  shorttitle = {Metadata {{Standard}} for {{Continuous Preservation}}, {{Discovery}}, and {{Reuse}} of {{Research Data}} in {{Repositories}} by {{Higher Education Institutions}}},
