@article{ingavelez-guerrauameeslr2023,
  title = {The Use of Accessibility Metadata in E-Learning Environments: A Systematic Literature Review},
  shorttitle = {The Use of Accessibility Metadata in E-Learning Environments},
  author = {{Ingav{\'e}lez-Guerra}, Paola and {Ot{\'o}n-Tortosa}, Salvador and {Hilera-Gonz{\'a}lez}, Jos{\'e} and {S{\'a}nchez-Gord{\'o}n}, Mary},
  year = {2023},
  month = jun,
  journal = {Universal Access in the Information Society},
  volume = {22},
  number = {2},
